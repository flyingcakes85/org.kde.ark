{
    "id": "org.kde.ark",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.14",
    "sdk": "org.kde.Sdk",
    "command": "ark",
    "rename-icon": "ark",
    "finish-args": [
        "--device=dri",
        "--filesystem=host",
        "--share=ipc",
        "--socket=wayland",
        "--socket=x11",
        "--talk-name=org.a11y.Bus"
    ],
    "cleanup": [
        "*.a",
        "*.la",
        "/include",
        "/lib/pkgconfig",
        "/share/doc",
        "/share/man"
    ],
    "modules": [
        "ark-modules.json",
    ]
}
